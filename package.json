{
  "name": "cross-exchange-arbitrage-bot",
  "version": "0.1.0",
  "description": "跨交易所資金費率套利平台 - 自動偵測幣安和 OKX 的資金費率差異並執行套利交易",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "arbitrage-bot": "./dist/cli/index.js"
  },
  "scripts": {
    "postinstall": "prisma generate",
    "build": "prisma migrate deploy && next build",
    "dev": "tsx watch server.ts",
    "start": "tsx server.ts",
    "cli": "tsx src/cli/index.ts",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:api": "pnpm build && node dist/test-api.js",
    "lint": "next lint",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\" \"app/**/*.{ts,tsx}\" \"components/**/*.{ts,tsx}\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"app/**/*.{ts,tsx}\" \"components/**/*.{ts,tsx}\"",
    "db:migrate": "prisma migrate dev",
    "db:generate": "prisma generate",
    "db:studio": "prisma studio",
    "db:seed": "tsx prisma/seed.ts",
    "update-oi-symbols": "tsx src/scripts/update-oi-symbols.ts",
    "docker:up": "docker compose up -d",
    "docker:down": "docker compose down",
    "docker:logs": "docker compose logs -f"
  },
  "keywords": [
    "arbitrage",
    "funding-rate",
    "cryptocurrency",
    "trading-bot",
    "binance",
    "okx"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=8.0.0"
  },
  "dependencies": {
    "@binance/connector": "^3.6.1",
    "@prisma/client": "5.22.0",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@types/bcrypt": "^6.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "autoprefixer": "^10.4.21",
    "axios": "^1.7.7",
    "bcrypt": "^6.0.0",
    "ccxt": "^4.5.11",
    "chalk": "^5.6.2",
    "class-variance-authority": "^0.7.1",
    "cli-table3": "^0.6.5",
    "clsx": "^2.1.1",
    "commander": "^12.1.0",
    "date-fns": "^4.1.0",
    "decimal.js": "^10.6.0",
    "dotenv": "^16.4.5",
    "ioredis": "^5.4.1",
    "jsonwebtoken": "^9.0.2",
    "log-update": "^7.0.1",
    "lucide-react": "^0.548.0",
    "next": "^14.2.33",
    "next-themes": "^0.4.6",
    "p-limit": "^7.2.0",
    "pg": "^8.16.3",
    "pino": "^9.4.0",
    "pino-pretty": "^11.2.2",
    "postcss": "^8.5.6",
    "protobufjs": "^7.5.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "recharts": "^2.15.4",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "strip-ansi": "^7.1.2",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^3.4.18",
    "telegraf": "^4.16.3",
    "ws": "^8.18.0",
    "zod": "3"
  },
  "devDependencies": {
    "@playwright/test": "^1.56.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/decimal.js": "^7.4.3",
    "@types/node": "^20.16.11",
    "@types/react": "19.2.2",
    "@types/ws": "^8.5.12",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@vitest/coverage-v8": "^2.1.2",
    "@vitest/ui": "^2.1.2",
    "eslint": "^8.57.1",
    "eslint-config-next": "^16.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "jsdom": "^27.1.0",
    "playwright": "^1.56.1",
    "prettier": "^3.3.3",
    "prisma": "^5.20.0",
    "tailwindcss-animate": "^1.0.7",
    "tsx": "^4.19.1",
    "typescript": "^5.6.2",
    "vitest": "^2.1.2"
  }
}
